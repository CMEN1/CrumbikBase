<body class="fondoform" xmlns="http://www.w3.org/1999/html">
<div class="container">
  <div class="row">


      <h4 class="black-text center">Detalles de la Organizaci&oacute;n</h4>
    <div class="col s12 fondodiv">

      <div class="input-field col s6">
        <select disabled>
          <option value=""disabled selected>Oro</option>
          <option value="1">Bronce - Gratis</option>
          <option value="2">Plata - 50$</option>
          <option value="3">Oro - 200$</option>
        </select>
        <label>Tipo de plan</label>
      </div>


      <div class="input-field col s6">

        <select disabled>
          <option value="" disabled selected>Servicios</option>
          <option value="1">Producci&oacute;n</option>
          <option value="2">Compra - Venta</option>
          <option value="2">Servicios</option>
          <option value="3">Sin fines de Lucro</option>
        </select>
        <label>Tipo de Organizaci&oacute;n</label>
      </div>

      <div class="input-field col s6 ">
        <i class="material-icons prefix iconnegro">business</i>
        <input disabled value="2549856" id="iFiscalOrg" type="text" class="validate">
        <label for="iFiscalOrg">Identificaci&oacute;n Fiscal</label>
      </div>

      <div class="input-field col s6 ">
        <i class="material-icons prefix iconnegro ">business</i>
        <input disabled value="Tombola" id="nombreOrg" type="text" class="validate">
        <label for="nombreOrg">Nombre</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">location_on</i>
        <input disabled value="Venezuela" id="paisOrg" type="text" class="validate">
        <label for="paisOrg">Pa&iacute;s</label>
      </div>


      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">phone</i>
        <input disabled value="0251-2336052" id="telefonoOrg" type="tel" class="validate">
        <label for="telefonoOrg">Tel&eacute;fono</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">email</i>
        <input disabled value="tombola@gmail.com" id="emailAdmO" type="text" class="validate">
        <label for="emailAdmO">Email</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">web</i>
        <input disabled value="www.tombola.com.ve" id="pagWeb" type="text" class="validate">
        <label for="pagWeb">Pagina Web</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">web</i>
        <input disabled value="facebook.com/tombola" id="faceOrg" type="text" class="validate">
        <label for="faceOrg">Facebook</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">web</i>
        <input disabled value="twitter.com/tombola" id="twitOrg" type="text" class="validate">
        <label for="twitOrg">Twitter</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">web</i>
        <input disabled value="instagram.com/tombola" id="instaOrg" type="text" class="validate">
        <label for="instaOrg">Instagram</label>
      </div>



      <div class="file-field input-field col s6">
        <div class="btn black disabled">
          <span>Logo</span>
          <input type="file" multiple>
        </div>
        <div class="file-path-wrapper">
          <input disabled value="tombola.jpg" class="file-path validate" type="text" placeholder="Upload one or more files">
        </div>
      </div>



    </div>


    <div class="center col s12">
      <a class=" waves-effect btn center white-text waves-light" <%=  link_to "Atras", paginas_consultar_org_path, class: "white-text" %></a>

    </div>
  </div>
</div>


</div>

<script>
    $(document).ready(function() {
        $('select').material_select();
    });
    $('select').material_select('destroy');
</script>

</body>