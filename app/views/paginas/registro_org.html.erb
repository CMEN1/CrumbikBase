<body class="fondoform" xmlns="http://www.w3.org/1999/html">
<div class="container">
  <div class="row">

    <div class="col s12 ">
      <h4 class="black-text center">Registrar Organizaci&oacute;n</h4>

      <div class="input-field col s6">
        <select>
          <option value=""disabled selected>Seleccione su plan</option>
          <option value="1">Bronce - Gratis</option>
          <option value="2">Plata - 50$</option>
          <option value="3">Oro - 200$</option>
        </select>
        <label>Tipo de plan</label>
      </div>

      <div class="input-field col s6">
        <select>
          <option value=""disabled selected>Seleccione un tipo</option>
          <option value="1">Producci&oacute;n</option>
          <option value="2">Compra - Venta</option>
          <option value="2">Servicios</option>
          <option value="3">Sin fines de Lucro</option>
        </select>
        <label>Tipo de Organizaci&oacute;n</label>
      </div>

      <div class="input-field col s6 ">
        <i class="material-icons prefix iconnegro">business</i>
        <input id="iFiscalOrg" type="text" class="validate">
        <label for="iFiscalOrg">Identificaci&oacute;n Fiscal</label>
      </div>

      <div class="input-field col s6 ">
        <i class="material-icons prefix iconnegro ">business</i>
        <input id="nombreOrg" type="text" class="validate">
        <label for="nombreOrg">Nombre</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">location_on</i>
        <input id="paisOrg" type="text" class="validate">
        <label for="paisOrg">Pa&iacute;s</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">phone</i>
        <input id="telefonoOrg" type="tel" class="validate">
        <label for="telefonoOrg">Tel&eacute;fono</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">email</i>
        <input id="emailAdmO" type="text" class="validate">
        <label for="emailAdmO">Email</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">web</i>
        <input id="pagWeb" type="text" class="validate">
        <label for="pagWeb">Pagina Web</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">web</i>
        <input id="faceOrg" type="text" class="validate">
        <label for="faceOrg">Facebook</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">web</i>
        <input id="twitOrg" type="text" class="validate">
        <label for="twitOrg">Twitter</label>
      </div>

      <div class="input-field col s6">
        <i class="material-icons prefix iconnegro">web</i>
        <input id="instaOrg" type="text" class="validate">
        <label for="instaOrg">Instagram</label>
      </div>

      <div class="file-field input-field col s6">
        <div class="btn white-text black">
          <span>Logo</span>
          <input type="file" multiple>
        </div>
        <div class="file-path-wrapper">
          <input class="file-path validate black-text" type="text" placeholder="Upload one or more files">
        </div>
      </div>

      <div class="row">
        <div class="col s12 center">
          <a class=" waves-effect btn center white-text waves-light" <%=  link_to "Siguiente", paginas_registroAdministradorOrg_path, class: "white-text" %></a>
          <a class=" waves-effect btn center white-text waves-light" <%=  link_to "Atras", root_path, class: "white-text" %></a>
      </div>
      </div>
    </div>
  </div>

</div>

<script>
    $(document).ready(function() {
        $('select').material_select();
    });
</script>

<script>
    $('.datepicker').pickadate({
        selectMonths: true, // Creates a dropdown to control month
        selectYears: 10, // Creates a dropdown of 15 years to control year
        currentText: 'Hoy',
        firstDay: 1,
        dayNamesMin: ['Do','Lu','Ma','Mi','Ju','Vi','SÃ¡'],
        closeText: 'Cerrar'
    });

</script>
</body>